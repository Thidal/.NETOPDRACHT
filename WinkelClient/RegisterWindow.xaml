<Window x:Class="WinkelClient.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinkelClient"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="300" Width="300">
    <Grid>
        <Label Content="Username" HorizontalAlignment="Left" Margin="31,78,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Register" Content="Button" HorizontalAlignment="Left" Margin="106,180,0,0" VerticalAlignment="Top" Width="75" Click="Register_OK"/>
        <TextBox x:Name="UsernameText" HorizontalAlignment="Left" Height="23" Margin="144,82,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
