<Window x:Class="WinkelClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinkelClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="Login" Content="Login" HorizontalAlignment="Left" Margin="219,191,0,0" VerticalAlignment="Top" Width="75" Click="Login_Click"/>
        <TextBox x:Name="UserText" HorizontalAlignment="Left" Height="23" Margin="308,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="PassText" HorizontalAlignment="Left" Height="23" Margin="308,110,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="Username" Content="UserName" HorizontalAlignment="Left" Margin="55,47,0,0" VerticalAlignment="Top" Width="78"/>
        <Label x:Name="Password" Content="Password" HorizontalAlignment="Left" Margin="55,107,0,0" VerticalAlignment="Top" Width="78"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="219,253,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
